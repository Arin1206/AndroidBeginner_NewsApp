<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyApprelease" parent="Theme.Material3.DayNight">
        <!-- Primary Brand Color -->
        <item name="colorPrimary">@android:color/holo_red_dark</item>
        <item name="android:statusBarColor">@color/black</item>

    </style>
    <style name="roundimageview" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
        <item name="color">@color/white</item>
    </style>
    <style name="CNN" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:windowBackground">@drawable/cnn</item>
        <item name="android:statusBarColor">@color/black</item>
    </style>
    <style name="menu" parent="Theme.Material3.DayNight">
        <item name="android:actionBarTheme">@color/red</item>
        <item name="android:statusBarColor">@color/black</item>
    </style>
    <style name="Theme.MyApprelease" parent="Base.Theme.MyApprelease" />
</resources>